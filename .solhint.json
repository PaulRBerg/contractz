{
  "extends": "default",
  "rules": {
    "indent": [
      "error",
      2
    ],
    "bracket-align": false,
    "compiler-fixed": false,
    "function-max-lines": false,
    "no-complex-fallback": false,
    "no-empty-blocks": false,
    "no-inline-assembly": false,
    "no-simple-event-func-name": false,
    "two-lines-top-level-separator": false
  }
}
